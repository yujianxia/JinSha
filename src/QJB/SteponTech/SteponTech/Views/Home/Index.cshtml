@using SteponTech.Data.BaseModels;
@{
    Layout = null;
}
<html>

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>金沙遗址博物馆青教版</title>
    <link rel="stylesheet" type="text/css" href="/css/reset.css" />
    <link rel="shortcut icon" href="/img/common/favicon.ico" />
    <link rel="stylesheet" type="text/css" href="/css/index.css">
    <link rel="stylesheet" type="text/css" href="/css/common/common.css">
</head>
<body>
    <div class="header">
        <div class="head">
            <div class="versions">
                <a href="">
                    <img src="/img/common/Chinese.png" alt="">
                </a>
                <div>
                    <a href="">
                        <img src="/img/common/English.png" alt="">
                    </a>
                    <a href="">
                        <img src="/img/common/xueshu.png" alt="">
                    </a>
                </div>
            </div>
            <div class="nav">
                <a href="/Strategy/Intro" class="a1"></a>
                <a href="/PlayJinSha/YoungExperience" class="a2"></a>
                <a href="/News/NewsList" class="a4"></a>
                <a href="/JinShaChest/Index" class="a3"></a>
                <img src="/img/common/logo.gif" id="icon-logo" alt="logo" />
            </div>
            <div class="login">
               <div class="loginoptrator">
                   <a href="/Member/Login" class="signUp">
                       <img src="/img/common/signUp.png" alt="">
                   </a>
                   <a href="/Member/VIPInstructions" class="signIn">
                       <img src="/img/common/signIn.png" alt="">
                   </a>
               </div>
                <div class="infoOptrator">
                    <a class="users">
                        <img src="/img/common/users.png" alt="">
                    </a>
                    <a href="/VIP/MyInformation" class="myInfo">
                        <img src="/img/common/myInfo.png" alt="">
                    </a>
                    <a class="loginOut">
                        <img src="/img/common/loginOut.png" alt="">
                    </a>
                </div>
            </div>
        </div>
        <a href="/Home/Index" class="greenTeaching"></a>
    </div>
    <!--banner-->
    <div class="banner">
        <img src="/img/banner/banner.jpg" />
    </div>
    <!--走进金沙-->
    <div class="intoSand"></div>
    <!--wrapper-->
    <div class="wrapper">
        <div class="glass">
            <a href="/JinShaChest/JinShaResource"><img src="/img/index/download-icon.png" /></a>
        </div>
        <div id="Layer1">
            <img title="还原" style="cursor: hand" onClick="realsize();" height="60" src="/img/index/zoom.png" width="60">
            <img title="缩小" style="cursor: hand" onClick="smallit();" height="60" src="/img/index/zoom_out.png" width="60">
            <img title="放大" style="cursor: hand" onClick="bigit();" height="60" src="/img/index/zoom_in.png" width="60">
        </div>
        <!--地图-->
        <div class="box" onLoad="" oncontextmenu="return false" ondragstart="return false" style="overflow-y: hidden; overflow-x: hidden">
            <p>
                <br>
            </p>
            <div id="hiddenPic" style="z-index: 1; left: 0px; visibility: hidden; width: 100px;
        position: absolute; top: 200px; height: 100px;outline: 1px solid rebeccapurple">
                <img src="/img/index/map.png" border="0" name="images2">
            </div>
            <div class="dragAble" id="block1" onMouseOver="dragObj=block1; drag=1;" style="z-index: 10;
        left: 0px; width: 100%; position: absolute; top: 0; height: 0px" onMouseOut="" drag="0">
                <div class="img" style="zoom: 1" border="0" name="images1" id="images1">
                    <img src="/img/index/map.png" />
                    @if (ViewBag.Infos?.Count > 0)
                    {
                        List<InformationYoungView> infoList = ViewBag.Infos;
                        var mapa1 = infoList.FirstOrDefault(e => e.Title == "北大门");
                        if (mapa1 != null)
                        {
                            <a class="map-a1" id="@mapa1.Id.ToString()">
                                <img src="/img/index/nav_btn_1.png" />
                            </a>
                        }
                        var mapa2 = infoList.FirstOrDefault(e => e.Title == "陈列馆");
                        if (mapa2 != null)
                        {
                            <a class="map-a2" id="@mapa2.Id.ToString()">
                                <img src="/img/index/nav_btn_2.png" />
                            </a>
                        }
                        var mapa3 = infoList.FirstOrDefault(e => e.Title == "东大门");
                        if (mapa3 != null)
                        {
                            <a class="map-a3" id="@mapa3.Id.ToString()">
                                <img src="/img/index/nav_btn_3.png" />
                            </a>
                        }
                        var mapa4 = infoList.FirstOrDefault(e => e.Title == "金沙剧场");
                        if (mapa4 != null)
                        {
                            <a class="map-a4" id="@mapa4.Id.ToString()">
                                <img src="/img/index/nav_btn_4.png" />
                            </a>
                        }
                        var mapa5 = infoList.FirstOrDefault(e => e.Title == "南大门");
                        if (mapa5 != null)
                        {
                            <a class="map-a5" id="@mapa5.Id.ToString()">
                                <img src="/img/index/nav_btn_5.png" />
                            </a>
                        }
                        var mapa6 = infoList.FirstOrDefault(e => e.Title == "青少年教育体验区");
                        if (mapa6 != null)
                        {
                            <a class="map-a6" id="@mapa6.Id.ToString()">
                                <img src="/img/index/nav_btn_6.png" />
                            </a>
                        }
                        var mapa7 = infoList.FirstOrDefault(e => e.Title == "文物保护与修护中心");
                        if (mapa7 != null)
                        {
                            <a class="map-a7" id="@mapa7.Id.ToString()">
                                <img src="/img/index/nav_btn_7.png" />
                            </a>
                        }
                        var mapa8 = infoList.FirstOrDefault(e => e.Title == "文化交流中心");
                        if (mapa8 != null)
                        {
                            <a class="map-a8" id="@mapa8.Id.ToString()">
                                <img src="/img/index/nav_btn_8.png" />
                            </a>
                        }
                        var mapa9 = infoList.FirstOrDefault(e => e.Title == "中国文化遗产纪念雕塑");
                        if (mapa9 != null)
                        {
                            <a class="map-a9" id="@mapa9.Id.ToString()">
                                <img src="/img/index/nav_btn_9.png" />
                            </a>
                        }
                        var mapa10 = infoList.FirstOrDefault(e => e.Title == "乌木林");
                        if (mapa10 != null)
                        {
                            <a class="map-a10" id="@mapa10.Id.ToString()">
                                <img src="/img/index/nav_btn_10.png" />
                            </a>
                        }
                        var mapa11 = infoList.FirstOrDefault(e => e.Title == "遗迹馆");
                        if (mapa11 != null)
                        {
                            <a class="map-a11" id="@mapa11.Id.ToString()">
                                <img src="/img/index/nav_btn_11.png" />
                            </a>
                        }
                        var mapa12 = infoList.FirstOrDefault(e => e.Title == "玉石之路");
                        if (mapa12 != null)
                        {
                            <a class="map-a12" id="@mapa12.Id.ToString()">
                                <img src="/img/index/nav_btn_12.png" />
                            </a>
                        }
                        var mapa13 = infoList.FirstOrDefault(e => e.Title == "游客接待中心");
                        if (mapa13 != null)
                        {
                            <a class="map-a13" id="@mapa13.Id.ToString()">
                                <img src="/img/index/nav_btn_13.png" />
                            </a>
                        }
                    }
                </div>
            </div>
        </div>
        <!--遮罩层-->
        <div class="alert">
            <div class="mask">
                <div class="close"></div>
                <!--内容-->
                <div class="innerText">
                    <h3 id="titleId"></h3>
                    <p id="introId"></p>
                </div>
                <div class="pic">
                    <img id="photoId" class="pic1" src="" />
                </div>
            </div>
        </div>
        <div class="bottomGlass"></div>
    </div>
    <div class="footer">
        <div class="mark"></div>
        @if (ViewBag.Footer != null)
            {
            <ul class="foot">
                <li>
                    <p>电话：@ViewBag.Footer.Phone</p>
                    <p class="footText">地址：@ViewBag.Footer.Address</p>
                    <div class="code wechatcode">
                        <img src="@ViewBag.Footer.WeChart" alt="" class="footText">
                        <p class="footWeixin">
                            <img src="/img/common/weixin_2.png" alt="">
                            <span>关注官方微信</span>
                        </p>
                    </div>
                    <div class="code weibocode">
                        <img src="@ViewBag.Footer.WeChart" alt="" class="footText">
                        <p class="footWeixin">
                            <img src="/img/common/weibo-icon.png" alt="">
                            <span>关注官方微博</span>
                        </p>
                    </div>
                </li>
                <li class="foot-li2">
                    <a href="http://map.baidu.com/?newmap=1&s=inf%26uid%3Ded4f5b58efb42cd248cc4af2%26wd%3D%E9%87%91%E6%B2%99%E9%81%97%E5%9D%80+%E7%99%BE%E5%BA%A6%E5%9C%B0%E5%9B%BE%26all%3D1%26c%3D75&from=alamap&tpl=map_singlepoint" target="_blank">
                        动态地图导航
                        <img src="/img/common/position.png" alt="">
                    </a>
                    <p class="group">社教QQ1群号:@ViewBag.Footer.QQGroup</p>
                    <p class="group2">社教QQ2群号:@ViewBag.Footer.QQGroup2</p>
                </li>
                <li>
                    <p>游览帮助</p>
                    @if (ViewBag.Infomations?.Count > 0)
                    {
                        <div class="group">
                            <a href="/Strategy/ValuableBook#@ViewBag.Infomations[0].Id">@ViewBag.Infomations[0].Title</a>
                            <a href="/Strategy/ValuableBook#@ViewBag.Infomations[1].Id">@ViewBag.Infomations[1].Title</a>
                        </div>
                        <div class="service">
                            <a href="/Strategy/ValuableBook#@ViewBag.Infomations[2].Id">@ViewBag.Infomations[2].Title</a>
                            <a href="/Strategy/ValuableBook#@ViewBag.Infomations[3].Id">@ViewBag.Infomations[3].Title</a>
                        </div>
                    }
                    <p class="number"><span>金沙遗址博物馆版权所有</span><span>@ViewBag.Footer.RegisterNo</span></p>
                </li>
            </ul>
        }
    </div>
</body>
<script src="/js/jquery.min.js" type="text/javascript" charset="utf-8"></script>
<script src="/css/common/common.js"></script>
<script src="/js/index.js" type="text/javascript" charset="utf-8"></script>
<script src="/js/drag_map.js" type="text/javascript" charset="utf-8"></script>

</html>
