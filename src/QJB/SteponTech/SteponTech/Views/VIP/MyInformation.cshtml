
@{
    Layout = null;
}

<html>

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>金沙遗址博物馆青教版</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="shortcut icon" href="/img/common/favicon.ico" />
    <link rel="stylesheet" type="text/css" href="/css/common/common.css">
    <link rel="stylesheet" type="text/css" href="/css/styles.css" media="all">
    <link rel="stylesheet" type="text/css" href="/css/vip_information.css">
</head>
<body>
    <div class="header">
        <div class="head">
            <div class="versions">
                <a href="">
                    <img src="/img/common/Chinese.png" alt="">
                </a>
                <div>
                    <a href="">
                        <img src="/img/common/English.png" alt="">
                    </a>
                    <a href="">
                        <img src="/img/common/xueshu.png" alt="">
                    </a>
                </div>
            </div>
            <div class="nav">
                <a href="/Strategy/Intro" class="a1"></a>
                <a href="/PlayJinSha/YoungExperience" class="a2"></a>
                <a href="/News/NewsList" class="a4"></a>
                <a href="/JinShaChest/Index" class="a3"></a>
                <img src="/img/common/logo.gif" id="icon-logo" alt="logo" />
            </div>
            <div class="login">
                <div class="loginoptrator">
                    <a href="/Member/Login" class="signUp">
                        <img src="/img/common/signUp.png" alt="">
                    </a>
                    <a href="/Member/VIPInstructions" class="signIn">
                        <img src="/img/common/signIn.png" alt="">
                    </a>
                </div>
                <div class="infoOptrator">
                    <a class="users">
                        <img src="/img/common/users.png" alt="">
                    </a>
                    <a href="/VIP/MyInformation" class="myInfo">
                        <img src="/img/common/myInfo.png" alt="">
                    </a>
                    <a class="loginOut">
                        <img src="/img/common/loginOut.png" alt="">
                    </a>
                </div>
            </div>
        </div>
        <a class="greenTeaching"></a>
    </div>
    <div class="empty"></div>
    <div class="content">
        <div class="leaf">

        </div>
        <div class="form">
            <div class="label">
                <div class="innerText">
                    <a>
                        <img src="/img/common/present.png" alt="">
                    </a>
                    <a href="/Home">
                        <img src="/img/common/home.png" alt="">
                    </a>
                    <a>
                        <img src="/img/vip_information/information_1.png" alt="">
                    </a>
                </div>
            </div>
            <div class="rule">
                <div class="choose">
                    <a href="/VIP" class="chooseA_2">
                        <img src="/img/common/rule.png" alt="">
                    </a>
                    <a href="/VIP/MyActivity" class="chooseA_1">
                        <img src="/img/common/activity .png" alt="">
                    </a>
                    <a href="/VIP/MyIntegral" class="chooseA_2">
                        <img src="/img/common/integral.png" alt="">
                    </a>
                    <a href="/VIP/MyExchange" class="chooseA_3">
                        <img src="/img/common/exchange.png" alt="">
                    </a>
                    <a>
                        <img src="/img/vip_information/information.png" alt="">
                    </a>
                </div>
                <div class="information">
                    <div class="informationFrom">
                        <p>会员卡号：<span id="mycard"></span></p>
                        <p>会员等级：<span id="mylevel"></span></p>
                        <p>会员姓名：<span id="myname"></span></p>
                        <p>会员积分：<span id="myscore"></span></p>
                        <p>分组信息：<span id="mygroup"></span></p>
                        <p>昵称： <input type="text" readonly="readonly" id="name" /><span class="modify"><img src="/img/vip_information/pencil.png" alt=""> 修改</span><i></i></p>
                        <p>手机号码：<input type="text" readonly="readonly" id="tel" /></p>
                        <p>邮箱： <input type="text" readonly="readonly" id="email" /></p>
                        <p>地址： <input type="text" readonly="readonly" id="addr" /><span class="modify"><img src="/img/vip_information/pencil.png" alt=""> 修改</span><i></i></p>
                        @*<p>
                            性别：
                            <input type="radio" checked="checked" id="sex1" name="sex" value="男"><label for="sex1">男</label>
                            <input type="radio" id="sex2" name="sex"><label for="sex2" value="女">女</label>
                        </p>*@
                        @*<div class="iDate date">
                            <span class="dataSpan">生日：</span>
                            <input type="text">
                            <button type="button" class="addOn"></button>
                        </div>*@
                        <button class="save">
                            <img src="/img/vip_information/save.png" alt="">
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="footer">
        <div class="mark"></div>
        @if (ViewBag.Footer != null)
        {
            <ul class="foot">
                <li>
                    <p>电话：@ViewBag.Footer.Phone</p>
                    <p class="footText">地址：@ViewBag.Footer.Address</p>
                    <div class="code wechatcode">
                        <img src="@ViewBag.Footer.WeChart" alt="" class="footText">
                        <p class="footWeixin">
                            <img src="/img/common/weixin_2.png" alt="">
                            <span>关注官方微信</span>
                        </p>
                    </div>
                    <div class="code weibocode">
                        <img src="@ViewBag.Footer.WeChart" alt="" class="footText">
                        <p class="footWeixin">
                            <img src="/img/common/weibo-icon.png" alt="">
                            <span>关注官方微博</span>
                        </p>
                    </div>
                </li>
                <li class="foot-li2">
                    <a href="http://map.baidu.com/?newmap=1&s=inf%26uid%3Ded4f5b58efb42cd248cc4af2%26wd%3D%E9%87%91%E6%B2%99%E9%81%97%E5%9D%80+%E7%99%BE%E5%BA%A6%E5%9C%B0%E5%9B%BE%26all%3D1%26c%3D75&from=alamap&tpl=map_singlepoint" target="_blank">
                        动态地图导航
                        <img src="/img/common/position.png" alt="">
                    </a>
                    <p class="group">社教QQ1群号:@ViewBag.Footer.QQGroup</p>
                    <p class="group2">社教QQ2群号:@ViewBag.Footer.QQGroup2</p>
                </li>
                <li>
                    <p>游览帮助</p>
                    @if (ViewBag.Infomations?.Count > 0)
                    {
                        <div class="group">
                            <a href="/Strategy/ValuableBook#@ViewBag.Infomations[0].Id">@ViewBag.Infomations[0].Title</a>
                            <a href="/Strategy/ValuableBook#@ViewBag.Infomations[1].Id">@ViewBag.Infomations[1].Title</a>
                        </div>
                        <div class="service">
                            <a href="/Strategy/ValuableBook#@ViewBag.Infomations[2].Id">@ViewBag.Infomations[2].Title</a>
                            <a href="/Strategy/ValuableBook#@ViewBag.Infomations[3].Id">@ViewBag.Infomations[3].Title</a>
                        </div>
                    }
                    <p class="number"><span>金沙遗址博物馆版权所有</span><span>@ViewBag.Footer.RegisterNo</span></p>
                </li>
            </ul>
        }
    </div>
</body>
<script src="/js/jquery.min.js" type="text/javascript"></script>
<script type="text/javascript" src="/css/common/common.js"></script>
<script type="text/javascript" src="/js/moment.js"></script>
<script src="/js/vip/vip_information.js" type="text/javascript" charset="utf-8"></script>
<script type="text/javascript" src="/js/bootstrap-datetimepicker.js"></script>
<script>
  $(document).ready(function(){
    // date time picker
    if($(".iDate.full").length>0){
      $(".iDate.full").datetimepicker({
        locale: "zh-cn",
        format: "YYYY-MM-DD a hh:mm",
        dayViewHeaderFormat: "YYYY年 MMMM"
      });
    }
    if($(".iDate.date").length>0){
      $(".iDate.date").datetimepicker({
        locale:"zh-cn",
        format:"YYYY-MM-DD",
        dayViewHeaderFormat:"YYYY年 MMMM"
      });
    }
  })
</script>
</html>