                @if (ViewBag.Colunms != null)
                {
                    foreach (var item in ViewBag.Colunms)
                    {
                        if (item.Sorting == 1)
                        {
                            <li  id="nav-one">@item.Name</li>
                        }
                        else if (item.Sorting == 2)
                        {
                            <li id="nav-two">@item.Name</li>

