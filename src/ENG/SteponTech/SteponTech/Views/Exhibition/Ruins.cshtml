                        }

                    }

                }
            </ul>
        </div>
        @if (ViewBag.Colunms != null)
        {




            <div id="Sacrifice" style="display:block">
                <div class="sacrifice row">
                    <div id='sacrifice-video' class="sands-introduce-img">
                        <div class='sands-introduce-imgMask img-responsive' style="overflow:hidden">
                            @{
                                var videosrc = "/upload/Information/" + ViewBag.Sacrifice.Id + "/" + ViewBag.Sacrifice.FileName + "";
                                <video style="height:100%;" controls="controls" loop="loop" preload="auto" src='@videosrc'></video>
                            }
                        </div>
                    </div>
                    <div class="sacrifice-info">
                        @Html.Raw(ViewBag.Sacrifice.Content)
                    </div>
                </div>
            </div>

